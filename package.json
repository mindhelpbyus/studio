{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 9002",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:security": "jest tests/security",
    "test:compliance": "jest tests/compliance",
    "test:performance": "jest tests/performance",
    "test:accessibility": "jest tests/accessibility",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run typecheck && npm run lint && npm run test:ci",
    "migrate:structure": "tsx scripts/migrate-to-healthcare-structure.ts",
    "compliance:check": "tsx scripts/compliance-check.ts",
    "compliance:report": "tsx scripts/compliance-report.ts",
    "security:audit": "tsx scripts/security-audit.ts",
    "security:scan": "npm audit && tsx scripts/security-scan.ts",
    "security:scan-deployed": "tsx scripts/security-scan-deployed.ts",
    "deploy": "tsx scripts/deploy.ts",
    "deploy:dev": "npm run deploy -- --environment development --providers aws",
    "deploy:staging": "npm run deploy -- --environment staging --providers aws,azure --parallel",
    "deploy:prod": "npm run deploy -- --environment production --providers aws,azure,gcp --rollback",
    "infra:synth": "cdk synth --app \"npx tsx infrastructure/cdk/bin/app.ts\"",
    "infra:deploy": "cdk deploy --app \"npx tsx infrastructure/cdk/bin/app.ts\"",
    "infra:destroy": "cdk destroy --app \"npx tsx infrastructure/cdk/bin/app.ts\"",
    "health-check": "tsx scripts/health-check.ts",
    "monitoring:setup": "tsx scripts/monitoring-setup.ts",
    "monitoring:dashboard": "tsx scripts/monitoring-dashboard.ts",
    "db:migrate": "tsx scripts/db-migrate.ts",
    "db:seed": "tsx scripts/db-seed.ts",
    "db:backup": "tsx scripts/db-backup.ts",
    "db:restore": "tsx scripts/db-restore.ts",
    "secrets:export": "tsx scripts/secrets-export.ts",
    "secrets:import": "tsx scripts/secrets-import.ts",
    "secrets:rotate": "tsx scripts/secrets-rotate.ts",
    "audit:generate": "tsx scripts/audit-report.ts",
    "audit:export": "tsx scripts/audit-export.ts",
    "test:gcp": "tsx scripts/test-gcp-integration.ts",
    "postinstall": "patch-package"
  },
